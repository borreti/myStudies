using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Trabalho_Grafos
{
    class PackDijkstra
    {
        public List<int>ListaCaminho { get; set; }
        public string mensagemFinal { get; set; }
        
        public PackDijkstra(List<int> ListaCaminho, string mensagemFinal)
        {
            this.ListaCaminho = ListaCaminho;
            this.mensagemFinal = mensagemFinal;
        }
    }
}
